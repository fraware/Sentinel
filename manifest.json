{
    "manifest_version": 3,
    "name": "Sentinel",
    "version": "0.2",
    "description": "Clean up the BS from your social media feeds",
    "icons": {
      "64": "icon512.png"
    },
    "content_scripts": [
      {
        "matches": ["https://www.linkedin.com/*"],
        "js": ["linkedin.js"]
      }
    ],
    "permissions": ["storage"],
    "options_ui": {
        "page": "options.html",
        "open_in_tab": true
      }
  }
  